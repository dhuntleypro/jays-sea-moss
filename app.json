{
  "expo": {
    "name": "Jays SM",
    "slug": "jays-sea-moss",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "jaysseamoss",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dhuntleyinc.jaysseamoss",
      "infoPlist": {
        "LSMinimumSystemVersion": "12.0",
        "CFBundleDisplayName": "Jays SM"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "eeb013ad-cada-4b60-9c12-7f3f17b44e41"
      }
    },
    "owner": "dhuntleypro"
  }
}
